{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.geonosis.shop.e.shop.geonosis",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.1",
      "toolsId": "b71896f2-4920-4d69-a9a1-493696575365"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.134.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.ui.layout": {},
        "sap.tnt": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.geonosis.shop.e.shop.geonosis.i18n.i18n"
        }
      },
      "catalog": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "./model/Catalog-test.json"
      },
      "welcome": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "./model/Welcome.json"
      },
      "featured": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "./model/FeaturedProducts.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.geonosis.shop.e.shop.geonosis.view",
        "async": true,
        "viewPath": "com.geonosis.shop.e.shop.geonosis.view"
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "Products",
          "pattern": "products/{subcategory}",
          "target": "TargetProducts"
        },
        {
          "name": "Cart",
          "pattern": "cart",
          "target": "TargetCart"
        },
        {
          "name": "Categories",
          "pattern": "categories",
          "target": "TargetCategories"
        },
        {
          "name": "Detail",
          "pattern": "detail",
          "target": "TargetDetail"
        },
        {
          "name": "NotFound",
          "pattern": "notFound",
          "target": "TargetNotFound"
        },
        {
          "name": "Test",
          "pattern": "Test",
          "target": "TargetTest"
        }
      ],
      "targets": {
        "TargetMain": {
          "id": "Main",
          "name": "Main"
        },
        "TargetProducts": {
          "name": "Products",
          "id": "Products",
          "viewLevel": 2,
          "controlAggregation": "pages",
          "controlId": "app",
          "viewType": "XML"
        },
        "TargetCategories": {
          "name": "Categories",
          "id": "Categories",
          "viewLevel": 2,
          "controlAggregation": "pages",
          "controlId": "app",
          "viewType": "XML"
        },
        "TargetCart": {
          "name": "Cart",
          "id": "Cart",
          "viewLevel": 2,
          "controlAggregation": "pages",
          "controlId": "app",
          "viewType": "XML"
        },
        "TargetDetail": {
          "name": "Detail",
          "id": "Detail",
          "viewLevel": 2,
          "controlAggregation": "pages",
          "controlId": "app",
          "viewType": "XML"
        },
        "TargetNotFound": {
          "name": "NotFound",
          "id": "NotFound",
          "viewLevel": 2,
          "controlAggregation": "pages",
          "controlId": "app",
          "viewType": "XML"
        },
        "TargetTest": {
          "name": "Test",
          "id": "Test",
          "viewLevel": 2,
          "controlAggregation": "pages",
          "controlId": "app",
          "viewType": "XML"
        }
      }
    },
    "rootView": {
      "viewName": "com.geonosis.shop.e.shop.geonosis.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "eShopGeonosis"
  }
}
